--port.tater.io:
  container_name: --port.tater.io
  image: docker-repository.tater.io:5000/apps/tater
  ports:
    - "8002:3000"
  restart: always
  environment:
    - MONGO_URL=mongodb://10.0.0.92:27017/--port
    - ROOT_URL=https://--port.tater.io
    - PORT=3000
  volumes_from:
      - shared-data:ro

