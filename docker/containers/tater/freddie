freddie.tater.io:
  container_name: freddie.tater.io
  image: docker-repository.tater.io:5000/apps/tater
  ports:
    - "8888:3000"
  restart: always
  environment:
    - MONGO_URL=mongodb://10.0.0.92:27017/freddie
    - ROOT_URL=https://freddie.tater.io
    - PORT=3000
  volumes_from:
    - shared-data:ro

