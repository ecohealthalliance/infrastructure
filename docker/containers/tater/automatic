automatic.tater.io:
  container_name: automatic.tater.io
  image: docker-repository.tater.io:5000/apps/tater
  ports:
    - "8020:3000"
  restart: always
  environment:
    - MONGO_URL=mongodb://10.0.0.92:27017/automatic
    - ROOT_URL=https://automatic.tater.io
    - PORT=3000
  volumes_from:
      - shared-data:ro

