clarkson.tater.io:
  container_name: clarkson.tater.io
  image: docker-repository.tater.io:5000/apps/tater
  ports:
    - "8003:3000"
  restart: always
  environment:
    - MONGO_URL=mongodb://10.0.0.92:27017/clarkson
    - ROOT_URL=https://clarkson.tater.io
    - PORT=3000
  volumes_from:
    - shared-data:ro

